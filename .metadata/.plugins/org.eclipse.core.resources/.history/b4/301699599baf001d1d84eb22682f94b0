package Naturalnumber;

import java.util.Scanner;

public class fib {
	
	public static int fib(int a , int b, int n) {
		if (n==0) {
			return 1;
		}
		int c= a+b;
		System.out.print(c);
		return fib(b, c, n-1);
	}

	public static void main(String[] args) {
		Scanner s = new Scanner (System.in);
		int n = s.nextInt();
		int a =0, b=1;
		System.out.print(a+" "+b+" ");
		    	int k= fib(a ,b , n-2 );
		    	System.out.print(k);

	}

}
