package ArrayPractce;

public class swapArray {

	public static void main(String[] args) {
		int [] arr1= {5,6,9,7,15,0,48};
		int temp;
		int l=arr1.length;
		int k= l/2;
		int i=0;
		for ( i=0;i<k;i++) {
			temp=arr1[i];
			arr1[i]=arr1[l-1-i];
			arr1[l-1-i]=temp;
			
		}
		for ( i=0;i<k;i++) {
			System.out.print(arr1[i]+" ");
		}
		

	}

}
