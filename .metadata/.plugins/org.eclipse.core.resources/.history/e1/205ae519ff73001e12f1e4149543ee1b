package Basic_String_Questions;

public class resverseString {
	
	
	// Cases where we reverse String.
	
	/* case1- "pw java" ---> "avaj wp"
	 * 2- "pwskills java ---> "java pwskills"
	 * 3- "pwskill java" ---> "avaj llikswp"
	 */

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// case 1-
		
		String s1= "Pw java";
		String result="";
		
		for (int i=s1.length()-1; i>=0; i--) {
			result+= s1.charAt(i);
		}
		System.out.println("case 1 :"+result);

	}

}
